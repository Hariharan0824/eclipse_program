package com.xwrokz.VegetableRunner;

import java.time.LocalDate;

public class PresidentDTO {

	private int id;
	private String name;
	private int age;
	private LocalDate durationStart;
	private LocalDate durationEnd;
	public PresidentDTO() {
		super();
		// TODO Auto-generated constructor stub
	}
	public PresidentDTO(int id, String name, int age, int durationStart, int durationEnd) {
		super();
		this.id = id;
		this.name = name;
		this.age = age;
		this.durationStart = durationStart;
		this.durationEnd = durationEnd;
	}
	@Override
	public String toString() {
		return "PresidentDTO [id=" + id + ", name=" + name + ", age=" + age + ", durationStart=" + durationStart
				+ ", durationEnd=" + durationEnd + "]";
	}
	@Override
	public int hashCode() {3...age..age..
		final int prime = 31;
		int result = 1;
		result = prime * result + age;
		result = prime * result + durationEnd;
		result = prime * result + durationStart;
		result = prime * result + id;
		result = prime * result + ((name == null) ? 0 : name.hashCode());
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		PresidentDTO other = (PresidentDTO) obj;
		if (age != other.age)
			return false;
		if (durationEnd != other.durationEnd)
			return false;
		if (durationStart != other.durationStart)
			return false;
		if (id != other.id)
			return false;
		if (name == null) {
			if (other.name != null)
				return false;
		} else if (!name.equals(other.name))
			return false;
		return true;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public int getDurationStart() {
		return durationStart;
	}
	public void setDurationStart(int durationStart) {
		this.durationStart = durationStart;
	}
	public int getDurationEnd() {
		return durationEnd;
	}
	public void setDurationEnd(int durationEnd) {
		this.durationEnd = durationEnd;
	}
	
	
}
